QComboBox::down-arrow:focus {
    image: url(qss:images/up-down_arrow_lighter.png);
}

QComboBox::down-arrow:off,
QComboBox::down-arrow:disabled {
    image: url(qss:images/up-down_arrow_disabled_dark.png);
}

